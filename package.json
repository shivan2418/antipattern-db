{
  "name": "antipattern-db",
  "version": "1.0.0",
  "description": "Static json file database",
  "keywords": [
    "database",
    "static",
    "json",
    "typescript",
    "query"
  ],
  "license": "MIT",
  "author": "Emil Elgaard",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "antipattern-db": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "npx tsx --test src/**/*.test.ts",
    "test:nested": "npx tsx --test src/test-nested-types.test.ts",
    "test:integration": "npx tsx src/test-integration.ts",
    "test:builder": "npx tsx src/builder/test-builder.test.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "jsontozod": "tsx src/jsontozod.ts",
    "db:build": "tsx src/cli/index.ts build",
    "db:validate": "tsx src/cli/index.ts validate",
    "db:info": "tsx src/cli/index.ts info"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "commander": "^12.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.6.2"
  }
}
